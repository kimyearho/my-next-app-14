{
    "parser": "typescript",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "vueIndentScriptAndStyle": false
}
